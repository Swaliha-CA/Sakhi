{
  "version": 2,
  "name": "sakhi-frontend",
  "buildCommand": "echo 'No build needed for static site'",
  "outputDirectory": ".",
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "/modules/(.*)",
      "dest": "/modules/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
